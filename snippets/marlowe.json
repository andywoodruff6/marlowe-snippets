{
    "choice": {
        "prefix": "choice",
        "body": [
            "(Choice",
            "\t(ChoiceId '$1'",
            "\t\t(Role '$2')) [",
            "\t(Bound ${3:#} ${4:#})])"
        ]
    },
    "deposit": {
        "prefix": "deposit",
        "body": [
            "(Deposit",
            "\t(Role ${1:to})",
            "\t(Role ${2:from})",
            "\t(Token ${3|token name, '' '' |})",
            "\t(ConstantParam ${4:value})"
        ]
    },
    "Pay": {
        "prefix": "pay",
        "body": [
            "(Pay",
            "\t(Role '$1'",
            "\t(Account $2)",
            "\t(Token ${3|token name, '' '' |})",
            "\t(ConstantParam '$4'"
        ]
    },
    "value": {
        "prefix": "value",
        "body": [
            "(${1| Constant, NegValu |}",
            "\t"
        ]
    },
    "when": {
        "prefix": "when",
        "body": [
            "When",
            "\t[Case ",
            "\t $1",
            "\t]",
            "\t(TimeParam 'Payment deadline')",
            "\tClose"
        ]
    }
    
}